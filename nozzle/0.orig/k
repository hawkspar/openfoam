FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inlet
    {
        type turbulentIntensityKineticEnergyInlet;
        intensity       0.05;
        value           uniform 1;
    }

    coflow
    {
        type turbulentIntensityKineticEnergyInlet;
        intensity       0.01;
        value           uniform 1;
    }

    far
    {
        type zeroGradient;
    }

    outlet
    {
        type zeroGradient;
    }

    wall
    {
        type kqRWallFunction;
        value           uniform 0;
    }

    front
    {
        type wedge;
    }

    back
    {
        type wedge;
    }

}


// ************************************************************************* //
