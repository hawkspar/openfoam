FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          GAMG;
        smoother        GaussSeidel;
        tolerance       1e-6;
        relTol          .001;
	nPreSweeps	0;
	nPostSweeps	2;
	cacheAgglomeration on;
	agglomerator	faceAreaPair;
	nCellsInCoarsestLevel 1000;
	mergeLevels	1;
}

    "(U|nuTilda)"
    {
        solver          PBiCGStab;
	preconditioner	DILU;
        tolerance       1e-8;
        relTol          .01;
    }
}

SIMPLE
{
    residualControl
    {
        p       1e-6;
        U       1e-8;
        nuTilda 1e-8;
    }
    consistent yes;
    nNonOrthogonalCorrectors 1;
    nCorrector 3;
    nOuterCorrectors 1;
}

relaxationFactors
{
    equations
    {
        U     .99; // 0.9 is more stable but 0.95 more convergent
        ".*"  .99; // 0.9 is more stable but 0.95 more convergent
    }
}

