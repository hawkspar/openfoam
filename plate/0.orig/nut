FoamFile
{
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 2e-5;

boundaryField
{
    inlet
    {
        type            freestream;
        freestreamValue $internalField;
    }

    symmetry_y { type symmetryPlane; }
    symmetry_z { type symmetryPlane; }

    infinity_x
    {
        type  		freestream;
        freestreamValue $internalField;
    }

    infinity_y
    {
        type  		freestream;
        freestreamValue $internalField;
    }

    infinity_z
    {
        type  		freestream;
        freestreamValue $internalField;
    }

    wall {
        type            fixedValue;
        value           uniform 0;    
    }
    
}

// ************************************************************************* //
