FoamFile
{
    format      ascii;
    class       dictionary;
    object      decomposeParDict;
}

matchTolerance 1E-3;

pointSync false;

patches
(
    {
        name inlet;

        patchInfo
        {
            type         patch;
            physicalType inlet;
        }

        constructFrom patches;

        patches (auto0);
    } 
    {
        name symmetry_y;

        patchInfo
        {
            type         symmetryPlane;
            physicalType symmetryPlane;
        }

        constructFrom patches;

        patches (auto1);
    }
    {
        name symmetry_z;

        patchInfo
        {
            type         symmetryPlane;
            physicalType symmetryPlane;
        }

        constructFrom patches;

        patches (auto2);
    }
    {
        name infinity_x;

        patchInfo
        {
            type         patch;
            physicalType outlet;
        }

        constructFrom patches;

        patches (auto3);
    }
    {
        name infinity_y;

        patchInfo
        {
            type         patch;
            physicalType outlet;
        }

        constructFrom patches;

        patches (auto4);
    }
    {
        name infinity_z;

        patchInfo
        {
            type         patch;
            physicalType outlet;
        }

        constructFrom patches;

        patches (auto5);
    }
    {
        name wall;

        patchInfo
        {
            type         wall;
            physicalType wall;
        }

        constructFrom patches;

        patches (auto6);
    }
